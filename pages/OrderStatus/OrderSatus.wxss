page {
  height: 100%;
  background-color: #F5F5F5;
}
.container { 
  display: flex;
  flex-direction: column;   /*每个元素从上往下放，各占一行*/  
  justify-content: space-around;  /*控制元素在垂直上的布局*/
  height: auto;
  padding-bottom: 25rpx;
}
.section {
  position: relative;
  height: 550rpx;
  background-color: #FFFFFF;
  border-radius: 10rpx;
  border: 3rpx solid #DDD; 
  padding-top: 15rpx;
  margin: 15rpx 20rpx auto 20rpx;
}
.line{ 
  font-size: 30rpx;
  color:#696969;
  margin-left: 35rpx;
  margin-top: 35rpx;
}
.btn{
  width: 300rpx;
  margin-left: 200rpx;
  position: absolute;
  bottom: 60rpx;
}
.key {
  font-size: 35rpx;
  margin-top: 35rpx;
  margin-left: 35rpx;
  font-weight: bold;
}